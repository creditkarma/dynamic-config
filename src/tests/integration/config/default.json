{
    "nullable_test": {
        "nullable": {
            "_source": "env",
            "_key": "NULLABLE",
            "_nullable": true
        },
        "not_nullable": {
            "_source": "env",
            "_key": "NOT_NULLABLE"
        }
    },
    "version": {
        "_source": "package",
        "_key": "version"
    },
    "project": {
        "id": {
            "name": "test-project",
            "ref": 987860
        },
        "health": {
            "control": "/check",
            "response": {
                "_source": "env",
                "_key": "HEALTH_RESPONSE",
                "_default": "GOOD"
            }
        }
    },
    "server": {
        "port": 8000,
        "host": "localhost"
    },
    "persistedQueries": {
        "databaseLookup": {
            "username": "root",
            "password": "root",
            "shardedDBHostsInfo": {
                "sharding": {
                    "client": {
                        "shard-info": {
                            "shard-count": 4,
                            "shard-map": [
                                {
                                    "virtual-start": 0,
                                    "virtual-end": 3,
                                    "destination": "localhost:4141"
                                }
                            ]
                        }
                    }
                }
            }
        }
    },
    "names": {
        "first": [ "Bob", "Helen", "Joe", "Jane" ],
        "last": [ "Smith", "Warren", "Malick" ]
    },
    "test-service": {
        "destination": "http://${HOST_NAME||localhost}:8080"
    }
}
